@model teatsite.Models.tNews

@{
    ViewBag.Title = Model.title;
    Layout = "~/Views/Shared/_Layout_admin1.cshtml";
}

<fieldset>
    <legend>Та энэ мэдээг устгах гэж байна.</legend>


    <div class="display-label">
        <h4>Мэдээний гарчиг</h4>
    </div>
    <div class="display-field">
        <h3>@Html.DisplayFor(model => model.title)</h3>
</div>

</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Устгах" /> |
        @Html.ActionLink("Жагсаалтруу буцах", "Index")
    </p>
}
